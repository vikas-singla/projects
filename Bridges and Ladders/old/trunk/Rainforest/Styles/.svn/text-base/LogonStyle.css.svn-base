.FormButtonStyle
{
    padding: 1px 3px 1px 4px;
    cursor: pointer;
    background: #004A7F;
    color: #fff;
    border: 1px solid #0e1f5b;
    border-left-color: #D9DFEA;
    border-top-color: #D9DFEA;
    font-size: 11px;
    font-weight: bold;
    width:55px;
    height: 22px;
}

.TextBoxSignInFieldStyle
{
    border-color: #C0C0C0;
    border-style: solid;
    border-width: 1px;
    font-size: 12px;
    color: #242424;
    width: 140px;
}

.LinkForgotPasswordStyle
{
    color: #004A7F;
    font-size: 10px;
    cursor: pointer;
}

.CheckboxRememberLoginStyle
{
    color: #004A7F;
    font-size: 10px;
}

.watermark_LoginFieldStyle
{
    border-color: #C0C0C0;
    border-style: solid;
    border-width: 1px;
    font-size: 12px;
    color: #A5A5A5;
    width: 140px;
}

.ColumnTextBoxLoginWidth
{
    padding-top: 2px;
    padding-bottom: 2px;
    padding-left: 5px;
    padding-right: 5px;
}
