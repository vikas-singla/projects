.addvendoroverlay {
	z-index:10000;
	background-color:#fff;
	width:775px;
	padding:0;
	margin:0;
}

.addvendoroverlay .content
{
    padding:10px;
    width:755px;
    min-height:100px;
    overflow:hidden;
    margin:0 auto;
}

.addvendoroverlay .overlaytitle, .addvendoroverlay .title
{
    width:730px;
    margin:0 auto 0 auto;
    font-variant:small-caps;
    font-size:16px;
    color:#444;
    text-align:left;
    padding:5px 2px 5px 2px;
    font-weight:bold;
}

.addvendoroverlay .title
{
    text-align:center;
    font-size:18px;
    color:#FF3030;
}

.addvendoroverlay .overlaytitle
{
    border-bottom:2px solid #FF3030;
}

.addvendoroverlay .formfields
{
    width:310px;
    margin:10px auto;
    background-color:#fff;
    padding:10px 40px 10px 10px;
    -moz-border-radius: 10px;
    -webkit-border-radius: 10px;
    border-radius: 10px;
    overflow:auto;
    display:block;
    float:left;
}

.addvendoroverlay .formfields .formfield, .addvendoroverlay .formfields .formfield .lbl, .addvendoroverlay .formfields .formfield .field
{
    width:310px;
    overflow:auto;
    display:block;
}

.addvendoroverlay .formfields .formfield .lbl
{
    padding-bottom:5px;
}

.addvendoroverlay .formfields .formfield .field #addVendorNameInput,
.addvendoroverlay .formfields .formfield .field #addVendorServiceNameInput,
.addvendoroverlay .formfields .formfield .field #addVendorLocationInput
{
    width:304px;
    font-size:14px;
}

.addvendoroverlay .formfields .formfield .field #addVendorCategoryInput
{
    width:310px;
    font-size:14px;
    padding: 4px 5px 3px 5px;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    border-radius: 3px;
    border: 1px solid #AAA;
}

.addvendoroverlay .btns
{
    clear:both;
    width:735px;
    text-align:center;
    padding:20px 0px 10px 0px;
}

.addvendoroverlay .addvendorimagesfrm
{
    width:355px;
    padding:10px;
    float:left;
}

.addvendoroverlay .addMediaForm
{
    width:335px;
    overflow:hidden;
    clear:both;
    padding:10px 10px 10px 10px;
    margin-left:20px;
    background-color: #F0F0F0;
    color: #7E7E7E;    
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    border-radius: 5px;
}

.addvendoroverlay .addMediaFormHeader
{
    width:335px;
    overflow:hidden;
    clear:both;
    padding:0 10px 5px 0px;
    margin-left:20px;
    background-color: #FFF;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    border-radius: 5px;
}

.addvendoroverlay .addMediaForm input, .addvendoroverlay .addMediaForm textarea
{
    width:325px;
}

.addvendoroverlay .addMediaForm .upload .uinput
{
    width:312px;
    padding:10px;    
    border:1px dashed #c9c9c9;
    margin-top:3px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    border-radius: 5px;
}

.addvendoroverlay .addMediaForm .upload .uinput ul
{
    list-style-type:square;
    margin-left:20px;
}

.addvendoroverlay .addMediaForm .upload
{
    clear:both;
    padding-top:10px;
}

.addvendoroverlay .addMediaForm .formfieldcenter
{
    width:100%;
    overflow:hidden;
    text-align:center;
    padding:2px 0px 2px 0px;
}
