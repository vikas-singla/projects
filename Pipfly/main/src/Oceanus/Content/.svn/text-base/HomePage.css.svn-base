.siteheadermsg
{
    width:520px;
    position:relative;
    margin:0 auto;
    position:relative;
    top:75px;
}

.siteheadermsg .stack1
{
    width:500px;
    display:block;
    padding: 15px 10px;
    text-align:center;
    background-color: #FFF9B1;
    background: -moz-linear-gradient(center bottom, #FFFCD1 0%, #FFF9B1 40%, #FFF9B1 100%);
    background: -o-linear-gradient(center bottom, #FFFCD1 0%, #FFF9B1 40%, #FFF9B1 100%);
    background: -webkit-linear-gradient(bottom, #FFFCD1 0%, #FFF9B1 40%, #FFF9B1 100%);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#FFFCD1',endColorstr='#FFF9B1');
    -moz-border-radius-bottomleft: 4px/*{cornerRadius}*/; -webkit-border-bottom-left-radius: 4px/*{cornerRadius}*/; border-bottom-left-radius: 4px/*{cornerRadius}*/;
    -moz-border-radius-bottomright: 4px/*{cornerRadius}*/; -webkit-border-bottom-right-radius: 4px/*{cornerRadius}*/; border-bottom-right-radius: 4px/*{cornerRadius}*/;
    border-left:1px solid #b0b0b0;
    border-right:1px solid #b0b0b0;
    border-bottom:1px solid #b0b0b0;
    position:relative;
    z-index:5;
    font-size:14px;
    line-height:20px;
    color: #474531;
    text-shadow: 0 1px rgba(255, 255, 255, 0.5);
}

.siteheadermsg .stack2
{
    position:absolute;
    top:2px;
    bottom:-3px;
    left:2px;
    right:2px;
    background-color: #FFF9B1;
    border-left:1px solid #b0b0b0;
    border-right:1px solid #b0b0b0;
    border-bottom:1px solid #b0b0b0;
    -moz-border-radius-bottomleft: 4px/*{cornerRadius}*/; -webkit-border-bottom-left-radius: 4px/*{cornerRadius}*/; border-bottom-left-radius: 4px/*{cornerRadius}*/;
    -moz-border-radius-bottomright: 4px/*{cornerRadius}*/; -webkit-border-bottom-right-radius: 4px/*{cornerRadius}*/; border-bottom-right-radius: 4px/*{cornerRadius}*/;
    z-index:4;
}

.siteheadermsg .stack3
{
    position:absolute;
    top:4px;
    bottom:-6px;
    left:4px;
    right:4px;
    background-color: #FFF9B1;
    border-left:1px solid #b0b0b0;
    border-right:1px solid #b0b0b0;
    border-bottom:1px solid #b0b0b0;    
    -moz-border-radius-bottomleft: 4px/*{cornerRadius}*/; -webkit-border-bottom-left-radius: 4px/*{cornerRadius}*/; border-bottom-left-radius: 4px/*{cornerRadius}*/;
    -moz-border-radius-bottomright: 4px/*{cornerRadius}*/; -webkit-border-bottom-right-radius: 4px/*{cornerRadius}*/; border-bottom-right-radius: 4px/*{cornerRadius}*/;
    z-index:3;
}

.striphomepageheader
{
    width: 100%;
    height: 45px;
    vertical-align: middle;
    background: #111;
    box-shadow: 0 1px 2px rgba(0,0,0,0.5);
    -moz-box-shadow: 0 1px 2px #202020;
    -webkit-box-shadow: 0 1px 2px #202020;
    position:fixed;
    z-index:999;
}

.btnundo:hover
{
    color: #444;
    font-weight: normal;
    border: 1px solid #ddd;
    background-color: #e9e9e9;
}

.navcontainer
{
    width:440px;
    position:absolute;
    top:10px
}

.striphomepageheader .stripheadercontainer
{
    width: 960px;
    margin: 0 auto;
}

.striphomepageheader .lavaLampBottomStyle li a, .striphomepageheader .lavaLampBottomStyle li a:hover, .striphomepageheader .lavaLampBottomStyle li a:active, .striphomepageheader .lavaLampBottomStyle li a:visited
{
    height: 25px;
    line-height: 25px;
    margin: auto 15px;
}

.striphomepageheader .lavaLampBottomStyle li a img
{
    border: none;
    margin-right: 5px;
    position: relative;
    top: 2px;
    float: left;
}

.calltoaction .btnyellowstyle
{
    position:absolute;
    top:26px;
    right:30px;
    font-size:15px;
}

.calltoaction, .calltoaction:hover, .calltoaction:visited, .calltoaction:active
{
    height:80px;
    width:900px;
    text-decoration:none;
    cursor:pointer;
    padding:0px 20px;
    display:block;
    overflow:hidden;
    text-align:left;
    position:relative;   
    -moz-border-radius: 10px;
    -webkit-border-radius: 10px;
    border-radius: 10px;
    background: url(/images/bluerbubbles.jpg) no-repeat top center;
}

.calltoaction .actiontext
{
    font-size:30px;
    cursor:pointer;
    font-family: HelveticaNeue, 'Helvetica Neue', HelveticaNeueRoman, HelveticaNeue-Roman, 'Helvetica Neue Roman', TeXGyreHerosRegular, Helvetica, Tahoma, Geneva, Arial, sans-serif;
    line-height:75px;
    text-shadow: #222 0px 1px 1px;
    font-weight:bold;
    color:#fff;
}

#userlogged, #userlogged:hover
{
    padding: 0px;
    height: 25px;
    line-height: 25px;
}

#userlogged .profileimg
{
    position: relative;
    top: 2px;
}

.city select
{
    width:390px;
    padding:5px;
    margin:10px 0px;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border: 1px solid #C0C0C0;
