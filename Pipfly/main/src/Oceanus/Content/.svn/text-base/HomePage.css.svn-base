    font-size: 16px;
    font-weight: bold;
    line-height: 20px;
}

.homepage .itemizedoverview .overviewitem .description
{
    width: 293px;
    margin-top: 10px;
    text-align: center;
    font-size: 14px;
    font-weight: normal;
    line-height: 20px;
}

.homepage .topsect
{
    width: 940px;
    position: relative;
    margin-top: 20px;
    min-height: 420px;
}

.homepage .topsect .fade
{
    height: 400px;
    width: 960px;
    z-index: -1;
    position: absolute;
    margin-top: -40px;
    display: block;
    background: white;
    background: -webkit-gradient(linear,center top,center bottom,from(#f4f4f4),to(white));
    background: -moz-linear-gradient(top,#f4f4f4,white);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#f4f4f4',endColorstr='#ffffff',GradientType=0);
    box-shadow: inset 0 1px 2px rgba(0,0,0,0.1),0 0 0 transparent,0 0 0 transparent,0 0 0 transparent,0 0 0 transparent;
    -moz-box-shadow: inset 0 1px 2px rgba(0,0,0,0.1),0 0 0 transparent,0 0 0 transparent,0 0 0 transparent,0 0 0 transparent;
    -webkit-box-shadow: inset 0 1px 2px rgba(0,0,0,0.1),0 0 0 transparent,0 0 0 transparent,0 0 0 transparent,0 0 0 transparent;
}

.homepage .topsect .fade .left
{
    width: 100px;
    height: 400px;
    position: absolute;
    top: 0;
    left: 0;
    background: -webkit-gradient(linear,left center,right center,from(white),to(rgba(255, 255, 255, 0)));
    background: -moz-linear-gradient(left center,white,rgba(255, 255, 255, 0));
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ffffffff',endColorstr='#00ffffff',GradientType=1);
}

.homepage .topsect .fade .right
{
    width: 100px;
    height: 400px;
    position: absolute;
    top: 0;
    right: 0;
    background: -webkit-gradient(linear,left center,right center,from(rgba(255, 255, 255, 0)),to(white));
    background: -moz-linear-gradient(left center,rgba(255, 255, 255, 0),white);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#00ffffff',endColorstr='#ffffffff',GradientType=1);
}

.homepage .topsect .bigsearchpanel
{
    width: 390px;
