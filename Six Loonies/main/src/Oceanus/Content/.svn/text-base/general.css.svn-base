/*----------------------------------------------------------
The base color for this template is #5c87b2. If you'd like
to use a different color start by replacing all instances of
#5c87b2 with your new color.
----------------------------------------------------------*/
body
{
    background-color: #fff;
    font-family: "lucida grande" ,tahoma,verdana,arial,sans-serif;
    font-size: 12px;
    color: #444;
    line-height: 1.4em;
    height: 100%;
    min-height: 100%;
    text-align: left;
    margin: 0;
}

.smalltext
{
    font-size: 10px;
}

.italictext
{
    font-style:italic;
}

img
{
    margin: 0;
    padding: 0;
    border: 0;
}

input
{
    color: #444;
}

textarea
{
    font-family: "lucida grande" ,tahoma,verdana,arial,sans-serif;
    line-height: 1.4em;
    color: #444;
}

.hiddenfield
{
    visibility: hidden;
    display:none;
}

.emptytextstyle
{
    color: #a9a9a9;
}

#bodycontainer
{
    min-height: 100%;
    height: 100%;
    position: relative;
    display: block;
    width: 100%;
}

.logoStyle
{
    text-align: left;
    width: 50px;
    max-height: 40px;
    height: 40px;
    overflow: hidden;
    float: left;
}

.logotextStyle
{
    text-align: left;
    width: 75px;
    float: left;
}

a, a:visited, a:active
{
    color: #106fb8;
    padding: 0px;
    border-width: 0px;
    font-size: 12px;
    text-decoration: none;
}

a:hover
{
    color: #106fb8;
    padding: 0px;
    border-width: 0px;
    font-size: 12px;
    text-decoration: underline;
    cursor: pointer;
}

.smalllink, .smalllink:visited, .smalllink:active
{
    color: #106fb8;
    padding: 0px;
    border-width: 0px;
    font-size: 10px;
    text-decoration: none;
}

.smalllink:hover
{
    color: #106fb8;
    padding: 0px;
    border-width: 0px;
    font-size: 10px;
    text-decoration: underline;
    cursor: pointer;
}

p, ul
{
    padding: 0;
    margin: 0;
    outline: 0;
    border: 0;
}

ul
{
    list-style: none;
}

