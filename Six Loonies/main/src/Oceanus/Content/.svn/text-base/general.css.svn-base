/* SITE NOTIFICATION  
----------------------------------------------------------*/
.error, .success
{
    font-size: 12px;
    margin: 10px auto;
    clear: both;
    width: 938px;
    height: 35px;
    line-height: 35px;
    font-weight: bold;
    text-align: left;
    -moz-border-radius: 10px;
    -webkit-border-radius: 10px;
    border-radius: 10px;
}

.error
{
    background-color: #FFF5F5;
    border: 1px solid #FE8392;
}

.success
{
    background-color: #F4FCEF;
    border: 1px solid #5DAE21;
}

.notificonalign
{
    position: relative;
    top: 5px;
    margin: 0px 5px;
    float: left;
}

