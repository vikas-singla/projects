.uploadifyButton, .uploadifyButton:hover, .uploadifyButton:visited, .btnredstyle, .btnredstyle:hover, .btnredstyle:visited, .btnstyle, .btnstyle:hover, .btnstyle:visited, .btnundo, .btnundo:hover, .btnundo:visited, .btnbluestyle, .btnbluestyle:hover, .btnbluestyle:visited, .btngreenstyle, btngreenstyle:hover, btngreenstyle:visited, .btnyellowstyle, .btnyellowstyle:hover, .btnyellowstyle:visited, .disabledbtn, disabledbtn:hover
