.userreview
{
    clear:both;
    width:679px;
    display:block;
    height:auto;
    padding-bottom:5px;
    position:relative;
}

.userreview .links
{
    padding:15px 0px 5px 0px;
}

.userreview .userstats
{
    color:#bd3b61;
    font-size:10px;
    font-weight:normal;
}

.userreview .date
{
    color:#909090;
    position:absolute;
    right:3px;
    top:0px;
    font-size:10px;
    font-weight:normal;
}

.userreview .links a
{
    padding:2px;
}

.userreview .column
{
    float:left;
    display:block;
    width:224px;
    padding:1px;
    overflow:hidden;
}

.userreview .audio
{
    clear:both;
    width:100%;
    padding-top:15px;
    text-align:center;
}

.userreview .column .header
{
    width:100%;
    background-color: #e9e9e9;
    font-weight:bold;
    display:block;
}

.userreview .column .header .text, .userreview .column .darkheader .text
{
    padding:5px 4px 5px 4px;
    font-size:11px;
}

.userreview .column .list
{
    clear:both;
    width:100%;
    font-weight:normal;
    display:block;
    margin-bottom:5px;    
}

.userreview .btns
{
    width:100%;
    text-align:right;
    padding:7px 0px 4px 0px;
}

.userreview .column .list .entry
{
    clear:both;
    display:block;
    width:212px;
    padding:7px 5px 7px 5px;
}

.userreview .column .list .entry .main, .userreview .column .list .editentry .main
{
    width:100%;
    float:left;
    text-align:left;
}

.userreview .comments
{
    width:100%;
    clear:both;
    padding-top:25px;
}

.userreview .reviewdimens
{
    width:100%;    
    overflow:hidden;
    text-align:left;
    display:block;
    float:left;
    padding-top:10px;
    padding-bottom:10px;
}

.userreview .reviewdimens .dimens
{
    width:217px;
    float:left;
    display:block;
    min-height:18px;
    padding-top:2px;
    padding-bottom:2px;
}

.userreview .reviewdimens .dimens .dimenstext
{
    float:left;
    display:block;
    text-align:left;
    width:126px;
    height:100%;
    font-size:12px;
    padding:0px 2px 0px 2px;
}

.userreview .reviewdimens .dimens .stars
{
    float:left;
    width:80px;
    display:block;
    height:100%;
    height:100%;
    vertical-align:middle;
    padding:0px 2px 0px 2px;
}